page{
	width: 100%;
	height: 100%;
	background-color:#edeef0;
}
.topTime{
	width: 100%;
	height: 85rpx;
	border-bottom:1rpx solid #eee;
	text-align: center;
	line-height: 85rpx;
	font-size: 35rpx;
	color: #666;
}
.box{
	width: 100%;
	height: auto;
	background-color: #fff;
	margin-bottom:30rpx;
	border-bottom:1rpx solid #eee;
	border-top:1rpx solid #eee;
}
.title{
	width: 100%;
	height: 68rpx;
	border-bottom: 1rpx solid #eee;
	color: #666;
	font-size: 30rpx;
	
	line-height: 68rpx;
}
.point{
	width: 20rpx;
	height: 20rpx;
	margin-left: 22rpx;
	display: inline-block;
	background-color: #ccc;
	border-radius: 50%;
}

.body{
	width: 100%;
	height: auto;
	background-color: #fff;
}
.detail{
	width: 100%;
	height: 60rpx;
	font-size: 30rpx;
	line-height: 60rpx;
	text-indent: 20rpx;
}
.detail text{
	margin-left: 40rpx;
}
.detail .difColor{
	margin-left:70rpx;
	color: #9cd1d7;
}