.detailList{
	width: 100%;
	height: 100%;
	
}
.detail{
	width: 100%;
	height: 195rpx;
	background-color: #fff;
	border-bottom: 1px solid #ccc;
}
.detailName{
	width: 100%;
	height: 55rpx;
	padding-top:20rpx;
}
.detailName .point{
	width: 42rpx;
	height:42rpx;
	display: inline-block;
	border-radius: 50%;
	background-color: #9cd1d7;
}
.detailName text{
	margin-left: 20rpx;
}

.detailBody{
	width: 100%;
	height: 65rpx;
	line-height: 65rpx;
	text-indent: 30rpx;
}
.detailBottom{
	width: 100%;
	height: 48rpx;
	font-size: 26rpx;
	color: #c7cbcc;
	text-indent: 30rpx;
}